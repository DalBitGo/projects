# Environment
ENVIRONMENT=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/storebridge

# Redis
REDIS_URL=redis://localhost:6379/0

# Domeggook API
DOMEGGOOK_API_KEY=your_api_key_here
DOMEGGOOK_API_URL=https://openapi.domeggook.com

# Naver Commerce API
NAVER_CLIENT_ID=your_client_id
NAVER_CLIENT_SECRET=your_client_secret
NAVER_API_URL=https://api.commerce.naver.com

# S3 / MinIO
S3_ENDPOINT_URL=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET_NAME=storebridge-images
S3_REGION=us-east-1

# Celery
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Monitoring
SENTRY_DSN=
PROMETHEUS_PORT=9090

# Rate Limiting
NAVER_MAX_TPS=2
NAVER_BURST_MAX=3
DOMEGGOOK_MAX_RPM=180
